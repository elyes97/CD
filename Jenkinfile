pipeline {
  environment {
    registry = "elyes97/cd"
    registryCredential = 'dockerhub'
  }
 agent any
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/elyes97/CD.git'
      }
    }
    stage('Building image') {
      steps{
        script {
          docker.build registry + ":$BUILD_NUMBER"
        }
      }
    }
  }
}


        
